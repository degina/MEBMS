<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.mebms.mebms.NewShinjilgeeFragment" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >
            <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/urhiincode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/urh_code" />

        <EditText
            android:id="@+id/urhiincodetext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="text" >

            <requestFocus />
        </EditText>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/urhiinterguun"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/urh_ezen_ner" />

        <EditText
            android:id="@+id/urhiinterguuntext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="text" >

        </EditText>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/ezencode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/ezencode" />

        <EditText
            android:id="@+id/ezencodetext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="text" >

        </EditText>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/ezenner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/ezenner" />

        <EditText
            android:id="@+id/ezennertext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="text" >

        </EditText>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/golomt"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="@string/golomt" />

        <Spinner
            android:id="@+id/golomt_spinner_shinjilgee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/aimag_shinjilgee"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="@string/aimag" />

        <Spinner
            android:id="@+id/aimag_spinner_shinjilgee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/sum_shinjilgee"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="@string/sum" />

        <Spinner
            android:id="@+id/sum_spinner_shinjilgee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />
    </LinearLayout>
    
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/turul_shinjilgee"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
			android:layout_gravity="fill"
            android:text="@string/turul" />

        <Spinner
            android:id="@+id/turul_spinner_shinjilgee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />
    </LinearLayout>
    
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/deej_shinjilgee"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="fill"
            android:text="@string/deej" />

        <Spinner
            android:id="@+id/deej_spinner_shinjilgee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />
    </LinearLayout>
    
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/arga_shinjilgee"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="fill"
            android:text="@string/arga" />

        <Spinner
            android:id="@+id/arga_spinner_shinjilgee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/urval_shinjilgee"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="fill"
            android:text="@string/urval" />

        <Spinner
            android:id="@+id/urval_spinner_shinjilgee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal" >

        <Button
            android:id="@+id/golomt_save"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Save" />


    </LinearLayout>
    
        </LinearLayout>
    </ScrollView>

    
</LinearLayout>
    